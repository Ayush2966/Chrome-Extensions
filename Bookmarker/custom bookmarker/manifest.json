{
    "manifest_version": 3,
    "version": "1.0",
    "name": "Bookmarker",
    "description": "A simple chrome extension to bookmark your favorite websites",
    "action": {
        "default_popup": "index.html",
        "default_icon": "icon.png"
    },
    "icons": {
        "128": "icon.png",
        "64": "icon.png",
        "48": "icon.png",
        "32": "icon.png",
        "16": "icon.png"
    },
    "permissions": [
        "tabs"
    ]
}